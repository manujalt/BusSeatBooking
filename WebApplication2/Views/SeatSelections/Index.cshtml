@model IEnumerable<WebApplication2.SeatSelection>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.seat)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fill)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.seatno)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.seat)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fill)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.seatno)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
